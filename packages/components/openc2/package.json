{
  "name": "@mdf.js/openc2",
  "version": "0.0.1",
  "description": "MMS - API - Observability",
  "keywords": [
    "NodeJS",
    "MMS",
    "API",
    "AJV"
  ],
  "repository": {
    "type": "git",
