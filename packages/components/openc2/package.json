    "@mdf.js/socket-server-provider": "*",
    "@mdf.js/utils": "*",
    "jsonwebtoken": "^9.0.0",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mdf.js/repo-config": "*",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.194",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1"
