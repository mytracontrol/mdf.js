    "mutants": "stryker run stryker.conf.js",
    "test": "jest --config ./jest.config.js"
  },
  "dependencies": {
    "@mdf.js/crash": "*",
    "@mdf.js/logger": "*",
    "@mdf.js/provider": "*",
    "@mdf.js/utils": "*",
    "kafkajs": "^2.2.2",
    "debug": "^4.3.4",
    "joi": "^17.6.3",
    "lodash": "^4.17.21",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mdf.js/repo-config": "*",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.188",
    "@types/uuid": "^8.3.4"
  },
  "engines": {
    "node": ">=16.14.2"
  },
