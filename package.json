{
  "name": "mms",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/**/*",
    "libs/**/*"
  ],
  "scripts": {
    "@:snyk:report": "snyk-to-html -i report.json -o report.html -a",
    "@:snyk:test": "snyk test --json-file-output=report.json --all-projects --fail-on=all && snyk monitor --all-projects",
    "build": "turbo run build",
    "check-dependencies": "turbo run check-dependencies --continue --concurrency=1",
    "clean": "rimraf \"./packages/*/*/*+(.tsbuildinfo)\" \"./{coverage,docs,mutations,tmp}\" \"./packages/**/*/{.turbo,dist,logs}\"",
    "clean:dist": "find . -name \"dist\" -type d -prune -exec rm -rf {} +",
    "clean:node_modules": "find . -name \"node_modules\" -type d -prune -exec rm -rf {} +",
    "compile": "tsc --build --clean && tsc --build",
    "dev": "turbo run dev",
    "docs": "yarn run docs:env && yarn run docs:typedoc && yarn run docs:media",
    "docs:media": ".config/copyMedia.sh",
    "docs:typedoc": "typedoc --options typedoc.json",
    "docs:env": "turbo run envDoc --force",
    "lint": "turbo run lint",
    "prettier": "prettier --config .prettierrc.js --write **/src/**/*.ts",
    "snyk": "yarn run @:snyk:test && yarn run @:snyk:report",
    "sort": "sort-package-json \"packages/*/*/package.json\" package.json",
    "test": "turbo run test licenses --concurrency=1"
  },
  "devDependencies": {
    "@mdf.js/repo-config": "*",
    "@stryker-mutator/core": "^8.2.5",
    "@stryker-mutator/jest-runner": "^8.2.5",
    "@stryker-mutator/typescript-checker": "^8.2.5",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^16.2.4",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "glob": "^10.3.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-html-reporter": "^3.10.1",
    "jest-junit": "^16.0.0",
    "jest-slow-test-reporter": "^1.0.0",
    "lerna": "^8.1.2",
    "license-checker": "^25.0.1",
    "npm-check": "^6.0.1",
    "prettier": "^3.2.5",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-lint": "^9.1.2",
    "remark-lint-unordered-list-marker-style": "^3.1.2",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "remark-toc": "^9.0.0",
    "rimraf": "^5.0.1",
    "snyk": "^1.1286.1",
    "snyk-to-html": "^2.5.0",
    "sort-package-json": "^2.9.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "turbo": "^1.13.0",
    "typedoc": "0.25.12",
    "typedoc-plugin-missing-exports": "^2.2.0",
    "typescript": "5.4.3"
  }
}
