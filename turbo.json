{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.ts"]
    },
    "mutants": {
      "dependsOn": ["test"],
      "outputs": [],
      "inputs": ["src/**/*.ts"]
    },
    "licenses": {
      "outputs": []
    },
    "doc": {
      "outputs": []
    }
  }
}
