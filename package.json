{
  "name": "mms",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/**/*",
    "libs/**/*"
  ],
  "scripts": {
    "@:snyk:report": "snyk-to-html -i report.json -o report.html -a",
    "@:snyk:test": "snyk test --json-file-output=report.json --all-projects --fail-on=all && snyk monitor --all-projects",
    "build": "turbo run build",
    "check-dependencies": "turbo run check-dependencies",
    "clean": "rimraf \"./packages/*/*/*+(.tsbuildinfo)\" \"./{coverage,docs,mutations,tmp}\" \"./packages/**/*/{.turbo,dist,logs}\"",
    "clean:dist": "find . -name \"dist\" -type d -prune -exec rm -rf {} +",
    "clean:node_modules": "find . -name \"node_modules\" -type d -prune -exec rm -rf {} +",
    "compile": "tsc --build --clean && tsc --build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "prettier": "prettier --config .prettierrc.js --write **/src/**/*.ts",
    "snyk": "yarn run @:snyk:test && yarn run @:snyk:report",
    "sort": "sort-package-json \"packages/*/*/package.json\" package.json",
    "test": "turbo run test licenses --concurrency=1"
  },
  "devDependencies": {
    "@mdf.js/repo-config": "*",
    "@stryker-mutator/core": "^6.3.0",
    "@stryker-mutator/jest-runner": "^6.3.0",
    "@stryker-mutator/typescript-checker": "^6.3.0",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^12.0.0",
    "eslint": "~8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "glob": "^8.0.3",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-html-reporter": "^3.7.0",
    "jest-junit": "^14.0.1",
    "jest-slow-test-reporter": "^1.0.0",
    "lerna": "^6.0.3",
    "license-checker": "^25.0.1",
    "npm-check": "^6.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "snyk": "^1.1060.0",
    "snyk-to-html": "^2.3.4",
    "sort-package-json": "^2.1.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "turbo": "^1.6.2",
    "typedoc": "0.23.21",
    "typescript": "^4.9.3"
  }
}
