{
  "name": "mms",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/**/*",
    "libs/**/*"
  ],
  "scripts": {
    "@:snyk:report": "snyk-to-html -i report.json -o report.html -a",
    "@:snyk:test": "snyk test --json-file-output=report.json --all-projects --fail-on=all && snyk monitor --all-projects",
    "build": "turbo run build",
    "check-dependencies": "turbo run check-dependencies --continue --concurrency=1",
    "clean": "rimraf \"./packages/*/*/*+(.tsbuildinfo)\" \"./{coverage,docs,mutations,tmp}\" \"./packages/**/*/{.turbo,dist,logs}\"",
    "clean:dist": "find . -name \"dist\" -type d -prune -exec rm -rf {} +",
    "clean:node_modules": "find . -name \"node_modules\" -type d -prune -exec rm -rf {} +",
    "compile": "tsc --build --clean && tsc --build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "prettier": "prettier --config .prettierrc.js --write **/src/**/*.ts",
    "snyk": "yarn run @:snyk:test && yarn run @:snyk:report",
    "sort": "sort-package-json \"packages/*/*/package.json\" package.json",
    "test": "turbo run test licenses --concurrency=1"
  },
  "devDependencies": {
    "@mdf.js/repo-config": "*",
    "@stryker-mutator/core": "^7.0.2",
    "@stryker-mutator/jest-runner": "^7.0.2",
    "@stryker-mutator/typescript-checker": "^7.0.2",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^13.0.2",
    "eslint": "~8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "glob": "^10.2.6",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-html-reporter": "^3.10.0",
    "jest-junit": "^16.0.0",
    "jest-slow-test-reporter": "^1.0.0",
    "lerna": "^7.0.2",
    "license-checker": "^25.0.1",
    "npm-check": "^6.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "snyk": "^1.1167.0",
    "snyk-to-html": "^2.3.7",
    "sort-package-json": "^2.4.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "turbo": "^1.9.9",
    "typedoc": "0.24.8",
    "typescript": "5.1.3"
  }
}
