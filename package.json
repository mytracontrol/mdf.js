{
  "name": "mms",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/**/*",
    "libs/**/*"
  ],
  "scripts": {
    "@:snyk:test": "snyk test --json-file-output=report.json --all-projects --fail-on=all && snyk monitor --all-projects",
    "@:snyk:report": "snyk-to-html -i report.json -o report.html -a",
    "snyk": "yarn run @:snyk:test && yarn run @:snyk:report",
    "clean:dist": "find . -name \"dist\" -type d -prune -exec rm -rf {} +",
    "clean:node_modules": "find . -name \"node_modules\" -type d -prune -exec rm -rf {} +",
    "clean": "rimraf \"{packages}/**/*+(.tsbuildinfo)\" \"./{coverage,docs,mutations,tmp}\" \"{packages}/**/{.turbo, dist,logs}\"",
    "compile": "tsc --build --clean && tsc --build",
    "getCredential": "vsts-npm-auth -config .npmrc",
    "prettier": "prettier --config .prettierrc.js --write **/src/**/*.ts",
    "sort": "sort-package-json \"packages/*/package.json\" \"libs/*/*/package.json\"",
    "build": "turbo run build",
    "test": "turbo run test mutants licenses --concurrency=1 && yarn run snyk",
    "lint": "turbo run lint",
    "dev": "turbo run dev"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^6.2.3",
    "@stryker-mutator/jest-runner": "^6.2.3",
    "@stryker-mutator/typescript-checker": "^6.2.3",
    "@types/jest": "29.0.3",
    "@types/node": "18.11.0",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^11.17.0",
    "eslint": "~8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "28",
    "jest-environment-jsdom": "28.1.1",
    "jest-html-reporter": "^3.7.0",
    "jest-junit": "^14.0.1",
    "jest-slow-test-reporter": "^1.0.0",
    "npm-check": "^6.0.1",
    "prettier": "^2.7.1",
    "sort-package-json": "^2.0.0",
    "ts-jest": "28",
    "ts-node": "10.9.1",
    "turbo": "^1.6.2",
    "typedoc": "^0.23.16",
    "glob": "^8.0.3",
    "typescript": "~4.8.4",
    "rimraf": "^3.0.2",
    "license-checker": "^25.0.1",
    "snyk": "^1.972.0",
    "snyk-to-html": "^2.3.4"
  }
}
