{
  "name": "@mdf/logger",
  "version": "0.0.1",
  "description": "MMS - API Logger - Enhanced logger library for mms artifacts",
  "keywords": [
    "NodeJS",
    "MMS",
    "API",
    "winston",
    "logger"
  ],
  "repository": {
    "type": "git",
    "url": "https://devopmytra.visualstudio.com/MytraManagementSystem/_git/MMS-microservices-nx",
    "directory": "libs/api/logger"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Mytra Control S.L.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rimraf \"{tsconfig.build.tsbuildinfo,dist}\"",
    "build": "yarn clean && tsc -p tsconfig.build.json",
    "test": "jest --config ./jest.config.js",
    "mutants": "stryker run stryker.conf.js",
    "licenses": "license-checker --start ./ --production --csv --out ../../../licenses/api/logger/licenses.csv --customPath ../../../.config/customFormat.json"
  },
  "dependencies": {
    "@mdf/crash": "*",
    "@mdf/utils": "*",
    "debug": "^4.3.4",
    "fluent-logger": "^3.4.1",
    "joi": "^17.6.3",
    "tslib": "^2.4.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/uuid": "^8.3.4"
  },
  "engines": {
    "node": ">=16.14.2"
  }
}
