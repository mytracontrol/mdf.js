{
  "name": "@mdf.js/provider",
  "version": "0.0.1",
  "description": "MMS - API Core - Provider wrapper",
  "keywords": [
    "NodeJS",
    "MMS",
    "API",
    "provider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mytracontrol/mdf.js.git",
    "directory": "packages/api/provider"
  },
  "license": "MIT",
  "author": "Mytra Control S.L.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.build.json",
    "check-dependencies": "npm-check",
    "clean": "rimraf \"{tsconfig.build.tsbuildinfo,dist}\"",
    "doc": "typedoc --options typedoc.json",
    "licenses": "license-checker --start ./ --production --csv --out ../../../licenses/api/provider/licenses.csv --customPath ../../../.config/customFormat.json",
    "mutants": "stryker run stryker.conf.js",
    "test": "jest --config ./jest.config.js"
  },
  "dependencies": {
    "@mdf.js/core": "*",
    "@mdf.js/crash": "*",
    "@mdf.js/logger": "*",
    "@mdf.js/utils": "*",
    "joi": "^17.6.3",
    "lodash": "^4.17.21",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^9.0.0"
  },
  "devDependencies": {
    "@mdf.js/repo-config": "*",
    "@types/lodash": "^4.14.190",
    "@types/uuid": "^8.3.4"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "@mdf.js/provider",
    "tsconfig": "./tsconfig.build.json"
  }
}
